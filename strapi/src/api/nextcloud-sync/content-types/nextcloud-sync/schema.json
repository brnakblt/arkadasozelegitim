{
    "kind": "collectionType",
    "collectionName": "nextcloud_syncs",
    "info": {
        "singularName": "nextcloud-sync",
        "pluralName": "nextcloud-syncs",
        "displayName": "Nextcloud Sync",
        "description": "Nextcloud kullanıcı eşleştirme"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "nextcloudUserId": {
            "type": "string",
            "required": true
        },
        "nextcloudDisplayName": {
            "type": "string"
        },
        "homeFolderPath": {
            "type": "string"
        },
        "quotaUsed": {
            "type": "biginteger",
            "default": "0"
        },
        "quotaTotal": {
            "type": "biginteger"
        },
        "syncStatus": {
            "type": "enumeration",
            "enum": [
                "active",
                "suspended",
                "error"
            ],
            "default": "active"
        },
        "lastSyncAt": {
            "type": "datetime"
        },
        "lastError": {
            "type": "text"
        },
        "user": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "plugin::users-permissions.user"
        }
    }
}