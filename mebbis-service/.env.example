# Arkada≈ü MEBBIS Service Configuration

# Server
PORT=4000
NODE_ENV=development

# MEBBIS Credentials (Required for automation)
MEBBIS_USERNAME=your_mebbis_username
MEBBIS_PASSWORD=your_mebbis_password

# Strapi Backend
STRAPI_URL=http://localhost:1337
STRAPI_API_TOKEN=your_strapi_api_token

# Redis (for job queue)
REDIS_URL=redis://localhost:6379

# Browser Automation
HEADLESS=true
BROWSER_TIMEOUT=30000

# Logging
LOG_LEVEL=info
